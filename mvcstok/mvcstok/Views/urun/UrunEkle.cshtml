
@{
    ViewBag.Title = "UrunEkle";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<form class="form-group" method="post">

    <div>
        <label>Ürün Adı </label>
        <input type="text" class="form-control" name="URUNADI" required="" />
    </div>
    <div>
        <label>Kategori Seçiniz  </label>
        <input type="text" class="form-control" name="URUNKATEGORİ" required="" />
    </div>
    <div>
        <label>Fiyat </label>
        <input type="text" class="form-control" name="FİYAT" required="" />
    </div>
    <div>
        <label>Marka </label>
        <input type="text" class="form-control" name="MARKA" required="" />
    </div>
    <div>
        <label>Stok Adet  </label>
        <input type="text" class="form-control" name="STOK " required="" />
